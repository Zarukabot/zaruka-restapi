  "paths": {
    "/api/orkut/createpayment": {
      "get": {
        "summary": "Create Payment",
        "parameters": [
          { "name": "apikey", "in": "query", "required": true, "schema": { "type": "string" } },
          { "name": "amount", "in": "query", "required": true, "schema": { "type": "number" } },
          { "name": "codeqr", "in": "query", "required": false, "schema": { "type": "string" } }
        ],
        "responses": { "200": { "description": "Payment created successfully" } }
      }
    },
    "/api/orkut/cekstatus": {
      "get": {
        "summary": "Cek Status Payment",
        "parameters": [
          { "name": "apikey", "in": "query", "required": true, "schema": { "type": "string" } },
          { "name": "merchant", "in": "query", "required": true, "schema": { "type": "string" } },
          { "name": "keyorkut", "in": "query", "required": true, "schema": { "type": "string" } }
        ],
        "responses": { "200": { "description": "Payment status result" } }
      }
    },

