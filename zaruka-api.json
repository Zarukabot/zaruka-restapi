{
  "openapi": "3.0.0",
  "info": {
    "title": "Zaruka API",
    "version": "1.0.0",
    "description": "Dokumentasi lengkap semua endpoint Zaruka API"
  },
  "servers": [
    { "url": "https://domainmu.com" }
